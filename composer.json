{
  "name": "victorstack-ai/wp-wayback-machine-link-fixer",
  "description": "WordPress plugin to replace broken external links with Wayback Machine snapshots.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "autoload": {
    "psr-4": {
      "WaybackLinkFixer\\": "wayback-link-fixer/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WaybackLinkFixer\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "phpcs --standard=PSR12 wayback-link-fixer/src tests",
    "test": "phpunit"
  }
}
